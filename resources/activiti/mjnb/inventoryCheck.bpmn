  <process id="inventoryCheck" name="盘点流程" isExecutable="true">
    <startEvent id="startevent1" name="Start" activiti:initiator="applyUserId"></startEvent>
    <userTask id="inventorycheckckzg" name="仓库主管人员审核">
      <extensionElements>
        <activiti:taskListener event="create" class="com.zoo.listener.activiti.InventoryCheckCKZGHandler"></activiti:taskListener>
        <activiti:taskListener event="complete" class="com.zoo.listener.activiti.InventoryCheckCKZGCompleteHandler"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="inventorycheckckzg"></sequenceFlow>
    <userTask id="inventorycheckkg" name="财务记账">
      <extensionElements>
        <activiti:taskListener event="create" class="com.zoo.listener.activiti.InventoryCheckGXKCHandler"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="inventorycheckckzg" targetRef="inventorycheckkg"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow3" sourceRef="inventorycheckkg" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow4" name="同意" sourceRef="exclusivegateway1" targetRef="endevent1">
      <extensionElements>
        <activiti:executionListener event="take" class="com.zoo.listener.activiti.InventoryCheckGXKCCompleteHandler"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${msg=='AGREE'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask1" name="订单调整"></userTask>
    <sequenceFlow id="flow5" name="驳回" sourceRef="exclusivegateway1" targetRef="usertask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${msg=='UNAGREE'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" name="重新提交" sourceRef="usertask1" targetRef="inventorycheckckzg"></sequenceFlow>
    <sequenceFlow id="flow7" name="作废" sourceRef="usertask1" targetRef="endevent1"></sequenceFlow>
  </process>
